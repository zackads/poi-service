{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "default": {},
  "required": [
    "name",
    "listEntry",
    "location",
    "grade",
    "hyperlink",
    "coordinates"
  ],
  "properties": {
    "name": {
      "id": "#/properties/name",
      "type": "string",
      "default": ""
    },
    "listEntry": {
      "id": "#/properties/listEntry",
      "type": "string",
      "default": ""
    },
    "location": {
      "id": "#/properties/location",
      "type": "string",
      "default": ""
    },
    "grade": {
      "id": "#/properties/grade",
      "type": "string",
      "default": "",
      "enum": ["I", "II", "II*"]
    },
    "hyperlink": {
      "id": "#/properties/hyperlink",
      "type": "string",
      "default": ""
    },
    "coordinates": {
      "id": "#/properties/coordinates",
      "type": "object",
      "default": {},
      "required": [
        "latitude",
        "longitude"
      ],
      "properties": {
        "latitude": {
          "id": "#/properties/coordinates/properties/latitude",
          "type": "number",
          "default": 0.0
        },
        "longitude": {
          "id": "#/properties/coordinates/properties/longitude",
          "type": "number",
          "default": 0.0
        }
      }
    }
  }
}